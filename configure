  case $BMPTOPNM in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_BMPTOPNM="$BMPTOPNM" # Let the user override the test with a path.
  ;;
  *)
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
